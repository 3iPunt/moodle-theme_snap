{{#formatsupportstoc}}
<nav id="course-toc">
    <div>
        <h2 id="toc-desktop-menu-heading">
             <span class=sr-only>Page</span>{{#str}}contents, theme_snap{{/str}}
        </h2>
        {{> theme_snap/course_toc_module_search}}
        <a id="toc-mobile-menu-toggle" title="{{#str}}contents, theme_snap{{/str}}" href="#course-toc"><i class="icon icon-close"></i></a>
    </div>

    {{#chapters}}
        {{> theme_snap/course_toc_chapters }}
    {{/chapters}}

    {{#footer}}
        {{> theme_snap/course_toc_footer }}
    {{/footer}}
</nav>
{{/formatsupportstoc}}
